#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/arina/Documents/homework01/node_modules/.pnpm/jest@30.2.0_@types+node@24._7e8c377217499d03f0638b5206c57b5d/node_modules/jest/bin/node_modules:/mnt/c/Users/arina/Documents/homework01/node_modules/.pnpm/jest@30.2.0_@types+node@24._7e8c377217499d03f0638b5206c57b5d/node_modules/jest/node_modules:/mnt/c/Users/arina/Documents/homework01/node_modules/.pnpm/jest@30.2.0_@types+node@24._7e8c377217499d03f0638b5206c57b5d/node_modules:/mnt/c/Users/arina/Documents/homework01/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/arina/Documents/homework01/node_modules/.pnpm/jest@30.2.0_@types+node@24._7e8c377217499d03f0638b5206c57b5d/node_modules/jest/bin/node_modules:/mnt/c/Users/arina/Documents/homework01/node_modules/.pnpm/jest@30.2.0_@types+node@24._7e8c377217499d03f0638b5206c57b5d/node_modules/jest/node_modules:/mnt/c/Users/arina/Documents/homework01/node_modules/.pnpm/jest@30.2.0_@types+node@24._7e8c377217499d03f0638b5206c57b5d/node_modules:/mnt/c/Users/arina/Documents/homework01/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/jest@30.2.0_@types+node@24._7e8c377217499d03f0638b5206c57b5d/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../.pnpm/jest@30.2.0_@types+node@24._7e8c377217499d03f0638b5206c57b5d/node_modules/jest/bin/jest.js" "$@"
fi
